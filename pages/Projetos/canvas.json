                "async () => {\n  let data = await ObterProjetosAPI.run().catch(res => {\n    showAlert(res);\n  });\n  for (var i = 0; i < data.length; i++) {\n    data[i].task = \"\";\n  }\n  await storeValue(\"ProjetosTable\", data);\n}"
